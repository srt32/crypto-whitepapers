<script type='text/javascript' src='https://edinarcoin.com/wp-includes/js/wp-embed.min.js?ver=4.6.1'></script>

<script>
    $(function (){
        $('.btn_scroll').click(function(event) {
            var id = $(this).attr("href");
            var target = parseInt($(id).offset().top);
            if ($(window).scrollTop() != target) {
                $('html, body').animate({
                    scrollTop: target
                }, 500);
            }
            event.preventDefault();
        });

        $('.tabs__btn').click(function (e) {
            e.preventDefault();
            $(this).closest('.tabs__list').find('.tabs__btn').removeClass('active');
            $(this).addClass('active');
            var tab = $(this).attr('href');
            $(this).closest('.tabs__wrap').find('.tabs__pane').not(tab).removeClass('active');
            $(tab).addClass('active');
        })
    })
</script>

</body>
</html>

